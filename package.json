{
  "name": "ask_hooks",
  "private": false,
  "version": "1.1.0",
  "type": "module",
  "main": "./dist/ask.cjs",
  "module": "./dist/ask.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/ask.cjs",
      "import": "./dist/ask.js",
      "types": "./dist/src/index.d.ts"
    },
    "./style": "./dist/style.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
